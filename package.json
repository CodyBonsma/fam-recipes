{
  "name": "fam-recipes",
  "version": "1.0.0",
  "description": "React app to hold family recipes",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm run start",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodyBonsma/fam-recipes.git"
  },
  "author": "Cody Bonsma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodyBonsma/fam-recipes/issues"
  },
  "homepage": "https://github.com/CodyBonsma/fam-recipes#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.14",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "query-string": "^6.13.8",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  }
}
